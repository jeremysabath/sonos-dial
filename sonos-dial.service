[Unit]
Description=Sonos Dial Controller
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
User=jeremysabath
Group=input
WorkingDirectory=/home/jeremysabath/sonos-dial
ExecStart=/home/jeremysabath/sonos-dial/.venv/bin/python main.py
Restart=always
RestartSec=5
TimeoutStopSec=10
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
